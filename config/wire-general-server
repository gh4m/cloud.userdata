
##
## wire-general-server
##

WG_CLOUDVPN_WGS1_LISTEN_PORT="51892"

## avoid using 192.168.122 NETWORK_BASE as it is used by virbr0

WG_CLOUDVPN_WGS1_NET_BASE="192.168.110"
WG_CLOUDVPN_WGS1_NET_MASK="24"
WG_CLOUDVPN_WGS1_IP_END="10"

## server dnscrypt-proxy blocking flag
WG_CLOUDVPN_WGS1_DNSCRYPT_PROXY_BLOCKING="YES"

## client peer file in config/<name>-client for each <name> in CLIENT_LIST
WG_CLOUDVPN_WGS1_CLIENT_LIST="wire-general phone pixel7"
